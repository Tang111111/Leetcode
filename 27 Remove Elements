//刷新了我对vector的认知
class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int len=nums.size()-1;
        int n=0;
        for(int i=0;i<=len;i++)
        {
            if(nums[i]!=val)
            {
                nums[n++]=nums[i];
            }       
        }
        return n;
    }
};
